{
  "name": "ditto-benchmark-artillery",
  "version": "1.0.0",
  "description": "Artillery-based benchmark tests for Eclipse Ditto",
  "main": "processor.ts",
  "scripts": {
    "build": "tsc",
    "test": "NODE_ENV=local artillery run artillery.ts --output results.json",
    "test:dist": "NODE_ENV=docker artillery run dist/artillery.js --output /results/results.json",
    "report": "node parse-results.js results.json"
  },
  "keywords": [
    "ditto",
    "benchmark",
    "artillery",
    "load-testing"
  ],
  "license": "EPL-2.0",
  "dependencies": {
    "@eclipse-ditto/ditto-javascript-client-node": "^3.9.0-M1.1",
    "artillery": "^2.0.27",
    "defu": "^6.1.4",
    "kafkajs": "^2.2.4",
    "yaml": "^2.8.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.0.0"
  },
  "overrides": {
    "glob": "^10.0.0",
    "inflight": "npm:@aashutoshrathi/inflight@^1.0.0",
    "@opentelemetry/api": "^1.9.0"
  }
}
