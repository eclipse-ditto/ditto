<project name="Eclipse Ditto">
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.3.0</version>
    </skin>
    <body>
        <head>
            <script type="text/javascript">
                $(document).ready(function () {
                var jsonStr = $("div.source pre").each(function(index, value) {
                var jsonStr = $(this).text()
                var jsonObj = JSON.parse(jsonStr);
                var jsonPretty = JSON.stringify(jsonObj, null, " ");
                $(this).text(jsonPretty);
                });

                $("div.source pre").addClass("prettyprint"); //.addClass("linenums");
                prettyPrint();
                }
                );
            </script>
        </head>
        <menu name="JSON Formats">
            <item name="Commands :: AMQP Bridge" href="commands-amqp-bridge.html">
                <item name="Query" href="commands-amqp-bridge-query.html"/>
                <item name="Modify" href="commands-amqp-bridge-modify.html"/>
                <item name="Responses" href="commands-amqp-bridge-responses.html">
                    <item name="Query" href="commands-amqp-bridge-responses-query.html"/>
                    <item name="Modify" href="commands-amqp-bridge-responses-modify.html"/>
                </item>
            </item>
            <item name="Errors :: AMQP Bridge" href="errors-amqp-bridge.html">
            </item>
        </menu>
    </body>
</project>
